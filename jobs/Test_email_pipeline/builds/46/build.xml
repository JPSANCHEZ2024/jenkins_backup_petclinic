    <result>SUCCESS</result>
    <script>pipeline {
    agent { label &apos;target3&apos; }

    stages {
        stage(&apos;Send Reports by email&apos;) {
            steps {
                script {
                     sh &apos;cp /home/azureuser/report_jmeter.jtl .&apos;
                     sh &apos;cp /home/azureuser/report_owasp_zap.xml .&apos;
                     sh &apos;cp /home/azureuser/report-task.txt .&apos;

                    // Stash the report files to make them available for the email step
                    stash includes: &apos;report_jmeter.jtl, report_owasp_zap.xml, report-task.txt&apos;, name: &apos;reports&apos;
                }
                
                // Unstash the reports before sending the email
                unstash &apos;reports&apos;
                
                emailext(
                    body: &apos;Attached are the reports of SonarQube, JMeter and OWASP-ZAP.&apos;,
                    subject: &apos;Reports: SonarQube, JMeter and OWASP-ZAP&apos;,
                    to: &apos;jpsg9868@gmail.com&apos;,
                    from: &apos;jpsg9868@gmail.com&apos;,
                    attachmentsPattern: &apos;report_jmeter.jtl, report_owasp_zap.xml, report-task.txt&apos;
                )
            }
        }
    }
}
</script>
